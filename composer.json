{
    "name": "wisingman/pelican-api-addon",
    "description": "This package brings an API interface to the application API in order to be able to create API tokens for a user",
    "keywords": [
        "PelicanAPIAddon"
    ],
    "homepage": "https://github.com/wisingman/pelican-api-addon",
    "license": "MIT",
    "authors": [
        {
            "name": "Max Moore (wisingman)",
            "email": "maxmooredotuk@gmail.com",
            "role": "Fork to Pelican Panel"
        },
        {
            "name": "Rene Roscher",
            "email": "roscher794@gmail.com",
            "role": "Original Developer"
        }
    ],
    "require": {
        "php": "^8.0|^8.1|^8.2|^8.3",
        "illuminate/support": "^9.0|^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "Wising\\PelicanAPIAddon\\": "src"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Wising\\PelicanAPIAddon\\PelicanApiAddonServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
